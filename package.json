{
  "name": "goszakup-automation",
  "version": "1.0.0",
  "description": "Сервис автоматизации подачи заявок на портале goszakup.gov.kz",
  "main": "dist/main.js",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch --preserveWatchOutput",
    "start:debug": "nest start --debug --watch --preserveWatchOutput",
    "start:prod": "node dist/main",
    "monitor:start": "ts-node -r tsconfig-paths/register src/main-monitor.ts",
    "monitor:start:dev": "ts-node -r tsconfig-paths/register src/main-monitor.ts",
    "monitor:start:prod": "node dist/main-monitor",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "keywords": [
    "goszakup",
    "automation",
    "ncanode",
    "ecp"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@nestjs/common": "^11.1.9",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.1.9",
    "@nestjs/platform-express": "^11.1.9",
    "@nestjs/schedule": "^6.0.1",
    "@types/ioredis": "^5.0.0",
    "@types/ws": "^8.18.1",
    "axios": "^1.13.2",
    "cheerio": "^1.1.2",
    "dotenv": "^16.6.1",
    "form-data": "^4.0.5",
    "https-proxy-agent": "^7.0.6",
    "ioredis": "^5.8.2",
    "node-cron": "^3.0.3",
    "open": "^11.0.0",
    "puppeteer-core": "^24.32.1",
    "reflect-metadata": "^0.2.2",
    "robotjs": "^0.6.0",
    "rxjs": "^7.8.2",
    "tough-cookie": "^4.1.4",
    "ws": "^8.18.3"
  },
  "devDependencies": {
    "@nestjs/cli": "^11.0.14",
    "@types/express": "^5.0.6",
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^24.10.1",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.9.3"
  }
}
