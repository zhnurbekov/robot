{
  "name": "goszakup-automation",
  "version": "1.0.0",
  "description": "Сервис автоматизации подачи заявок на портале goszakup.gov.kz",
  "main": "dist/main.js",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "cross-env NODE_OPTIONS='-r ./src/polyfills.js' nest start",
    "start:dev": "cross-env NODE_OPTIONS='-r ./src/polyfills.js' nest start --watch --preserveWatchOutput",
    "start:debug": "cross-env NODE_OPTIONS='-r ./src/polyfills.js' nest start --debug --watch --preserveWatchOutput",
    "start:prod": "cross-env NODE_OPTIONS='-r ./dist/polyfills.js' node dist/main",
    "monitor:start": "cross-env NODE_OPTIONS='-r ./src/polyfills.js' ts-node -r tsconfig-paths/register src/main-monitor.ts",
    "monitor:start:dev": "cross-env NODE_OPTIONS='-r ./src/polyfills.js' ts-node -r tsconfig-paths/register src/main-monitor.ts",
    "monitor:start:prod": "cross-env NODE_OPTIONS='-r ./dist/polyfills.js' node dist/main-monitor",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "keywords": [
    "goszakup",
    "automation",
    "ncanode",
    "ecp"
  ],
  "author": "",
  "license": "ISC",
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  },
  "dependencies": {
    "@nestjs/common": "^10.3.0",
    "@nestjs/config": "^3.1.1",
    "@nestjs/core": "^10.3.0",
    "@nestjs/platform-express": "^10.3.0",
    "@nestjs/schedule": "^4.0.0",
    "@types/ioredis": "^5.0.0",
    "@types/ws": "^8.18.1",
    "axios": "^1.4.0",
    "cheerio": "^0.22.0",
    "dotenv": "^16.6.1",
    "form-data": "^4.0.5",
    "https-proxy-agent": "^7.0.6",
    "ioredis": "^5.8.2",
    "node-cron": "^3.0.3",
    "open": "^10.1.0",
    "puppeteer-core": "^21.0.0",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.2",
    "tough-cookie": "^4.1.4",
    "web-streams-polyfill": "^4.0.0",
    "ws": "^8.18.3"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.2.1",
    "@types/express": "^4.17.21",
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^18.19.0",
    "cross-env": "^10.1.0",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.3.3"
  }
}
